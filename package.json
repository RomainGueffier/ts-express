{
    "name": "ts-express",
    "version": "1.0.0",
    "description": "Typescript Express Template",
    "main": "app.js",
    "license": "MIT",
    "scripts": {
        "build": "swc src -d dist",
        "build-dev": "yarn build && NODE_ENV=development node --inspect dist/app.js",
        "dev": "nodemon --ext ts --exec \"yarn build-dev\"",
        "start": "yarn build && NODE_ENV=production node dist/app.js"
    },
    "devDependencies": {
        "@swc/cli": "^0.1.52",
        "@swc/core": "^1.2.113",
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/node": "^16.11.11",
        "@types/uuid4": "^2.0.0",
        "@types/validator": "^13.7.0",
        "nodemon": "^2.0.15",
        "prisma": "^3.9.1",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.2"
    },
    "dependencies": {
        "@prisma/client": "^3.14.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "sqlite3": "^5.0.3",
        "uuid4": "^2.0.2"
    },
    "type": "module"
}
